:css
  .navbar-fixed-top .container {
    width: 100% !important;
  }

  .main-view {background:#fff; padding-bottom:0px;}
  .nav-tabs {margin-top:10px;}
  .tab-content {min-height: 618px; margin-left: 30px !important;}

.dashboards.span12
  %ul#tabs.nav.nav-tabs
    %li.core-metrics
      %a{:href => "#core"} Core
    %li
      %a{:href => "#moderators"} Moderators

    %span.domains
      %span.loading.hide= image_tag "ajax-loader.gif"
      - domains = {30 => '1 month', 60 => '2 months', 90 => '3 months', 180 => '6 months', 360 => '1 year'}
      - domains.each do |k,v|
        %a{domain: k}= v
        = "|" unless domains.keys.last == k

  .row
    .tab-content
      #core.dashboard.span12.tab-pane
        = render :partial => "core"
      #moderators.dashboard.span12.tab-pane.reloadable
        = render :partial => "moderators"

